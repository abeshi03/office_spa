<div class="review_user_box">
  <div class="user_name_box">
    <%= image_tag "user.jpg" %>
    <span class="review_user_name"><%= review.user.name %> さんの口コミ</span>
  </div>
  <div class="user_score">
    <p>評価  <%= review.score %></p>
    <span><%= time_ago_in_words(review.created_at)+"前" %></span>
  </div>
  <div class="review_description_box">
    <p class="review_description">
      <%= review.content %>
    </p>
  </div>
</div>